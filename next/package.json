{
  "name": "personal-finance-next",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3000",
    "prebuild": "node tools/select-prisma-schema.mjs",
    "build": "npm run prisma:generate && next build",
    "start": "next start -p 3000",
    "lint": "next lint",
    "prisma:generate": "prisma generate --schema=./prisma/schema.prisma",
    "prisma:migrate": "prisma migrate dev --name init --schema=./prisma/schema.prisma",
    "prisma:seed": "tsx prisma/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "^5.16.2",
    "date-fns": "^3.6.0",
    "next": "^14.2.5",
    "next-auth": "^4.24.11",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.12",
    "@types/node": "^20.11.30",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.2.5",
    "postcss": "^8.4.38",
    "prisma": "^5.16.2",
    "tailwindcss": "^4.0.0",
    "tsx": "^4.16.2",
    "typescript": "^5.4.5"
  }
}
